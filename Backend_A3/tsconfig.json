{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      //"outDir": "dist",
      "outDir": "./dist",
    "rootDir": "./src",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
      
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }